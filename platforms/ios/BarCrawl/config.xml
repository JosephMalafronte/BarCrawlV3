<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="BuildInfo">
        <param name="ios-package" value="CDVBuildInfo" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <name>BarCrawl</name>
    <description>
        BarCrawl v4.0
    </description>
    <author email="jgmalafronte@gmail.com" href="http://cordova.io">
        Joseph Malafronte
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="AppStore/AppIcon180.png" width="180" />
    <icon height="60" src="AppStore/AppIcon60.png" width="60" />
    <icon height="120" src="AppStore/AppIcon120.png" width="120" />
    <icon height="76" src="AppStore/AppIcon76.png" width="76" />
    <icon height="152" src="AppStore/AppIcon152.png" width="152" />
    <icon height="40" src="AppStore/AppIcon40.png" width="40" />
    <icon height="80" src="AppStore/AppIcon80.png" width="80" />
    <icon height="57" src="AppStore/AppIcon57.png" width="57" />
    <icon height="114" src="AppStore/AppIcon114.png" width="114" />
    <icon height="72" src="AppStore/AppIcon72.png" width="72" />
    <icon height="144" src="AppStore/AppIcon144.png" width="144" />
    <icon height="167" src="AppStore/AppIcon167.png" width="167" />
    <icon height="29" src="AppStore/AppIcon29.png" width="29" />
    <icon height="58" src="AppStore/AppIcon58.png" width="58" />
    <icon height="87" src="AppStore/AppIcon87.png" width="87" />
    <icon height="50" src="AppStore/AppIcon50.png" width="50" />
    <icon height="100" src="AppStore/AppIcon100.png" width="100" />
    <icon height="1024" src="AppStore/AppIcon1024.png" width="1024" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="CameraUsesGeolocation" value="false" />
</widget>
