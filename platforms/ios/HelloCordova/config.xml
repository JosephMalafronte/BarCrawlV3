<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="ImagePicker">
        <param name="ios-package" value="SOSPicker" />
    </feature>
    <name>BarCrawl</name>
    <description>
        BarCrawl v4.0
    </description>
    <author email="jgmalafronte@gmail.com" href="http://cordova.io">
        Joseph Malafronte
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon height="180" platform="ios" src="AppStore/AppIcon1024.png" width="180" />
    <icon height="60" platform="ios" src="AppStore/AppIcon1024.png" width="60" />
    <icon height="76" platform="ios" src="AppStore/AppIcon1024.png" width="76" />
    <icon height="152" platform="ios" src="AppStore/AppIcon1024.png" width="152" />
    <icon height="167" platform="ios" src="AppStore/AppIcon1024.png" width="167" />
    <icon height="29" platform="ios" src="AppStore/AppIcon1024.png" width="29" />
    <icon height="58" platform="ios" src="AppStore/AppIcon1024.png" width="58" />
    <icon height="87" platform="ios" src="AppStore/AppIcon1024.png" width="87" />
    <icon height="40" platform="ios" src="AppStore/AppIcon1024.png" width="40" />
    <icon height="80" platform="ios" src="AppStore/AppIcon1024.png" width="80" />
    <icon height="120" platform="ios" src="AppStore/AppIcon1024.png" width="120" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="1024" src="AppStore/AppIcon1024.png" width="1024" />
    <splash src="www/splash/ios/Default@2x~universal~anyany.png" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="CameraUsesGeolocation" value="false" />
</widget>
