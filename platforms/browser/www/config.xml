<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BarCrawl</name>
    <description>
        BarCrawl v4.0
    </description>
    <author email="jgmalafronte@gmail.com" href="http://cordova.io">
        Joseph Malafronte
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon height="180" platform="ios" src="AppStore/AppIcon1024.png" width="180" />
    <icon height="60" platform="ios" src="AppStore/AppIcon1024.png" width="60" />
    <icon height="76" platform="ios" src="AppStore/AppIcon1024.png" width="76" />
    <icon height="152" platform="ios" src="AppStore/AppIcon1024.png" width="152" />
    <icon height="167" platform="ios" src="AppStore/AppIcon1024.png" width="167" />
    <icon height="29" platform="ios" src="AppStore/AppIcon1024.png" width="29" />
    <icon height="58" platform="ios" src="AppStore/AppIcon1024.png" width="58" />
    <icon height="87" platform="ios" src="AppStore/AppIcon1024.png" width="87" />
    <icon height="40" platform="ios" src="AppStore/AppIcon1024.png" width="40" />
    <icon height="80" platform="ios" src="AppStore/AppIcon1024.png" width="80" />
    <icon height="120" platform="ios" src="AppStore/AppIcon1024.png" width="120" />
</widget>
